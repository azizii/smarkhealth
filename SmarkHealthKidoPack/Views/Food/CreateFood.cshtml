@model SmarkHealthKidoPack.ViewModel.Foodviewmode
@{
    ViewData["Title"] = "FoodView";
    Layout = "~/Views/Shared/AdminLayOut.cshtml";
}



<div class="container">
    <div class="card card-login mx-auto mt-5">
        <div class="card-header">Create food</div>
        <div class="card-body">
            <form enctype="multipart/form-data" asp-action="CreateFood">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label class="control-label">Category Name</label>
                    <a class="btn btn-success" style="width:50%" asp-controller="FoodCategories" asp-action="Create">Create an Food Category</a>
                    @Html.DropDownListFor(model => model.foodCategoryId, new SelectList(ViewBag.foodCategoryId, "Value", "Text"), new { @class = "dropdown-toggle btn btn-primary" })




                    @*<select asp-for="foodCategory.FoodCategoryName" class ="form-control" asp-items="ViewBag.foodCategoryId"></select>*@
                </div>



                <div class="form-group">
                    <label asp-for="foodName" class="control-label"></label>
                    <input asp-for="foodName" class="form-control" />
                    <span asp-validation-for="foodName" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="price" class="control-label"></label>
                    <input asp-for="price" class="form-control" />
                    <span asp-validation-for="price" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="foodCalories" class="control-label"></label>
                    <input asp-for="foodCalories" class="form-control" />
                    <span asp-validation-for="foodCalories" class="text-danger"></span>
                </div>
