
@model SmarkHealthKidoPack.Models.Mess
@{
    ViewData["Title"] = "myprofile";
    Layout = "~/Views/Shared/AdminLayOut.cshtml";
    var photoPath = "~/Images/" + (Model.photopath ?? "noimage.jpg");
}

<div class="header text-center">
    <h1>myprofile</h1>
</div>

<div class="container">

    <div class="row">



        <div class="col-md-4">
            <div class="card">
                <div class="card-header text-center">
                    <i class="fa fa-user"></i><strong class="card-title pl-2">@Model.MessName</strong>
                </div>

                <div class="card-body">
                    <div class="mx-auto d-block">
                        <img class="rounded-circle mx-auto d-block" style="width:17em; height:15em;" src="@photoPath" asp-append-version="true" />

                        <h5 class="text-sm-center mt-2 mb-1"></h5>

                        <div class="location text-sm-center"><i class="fa fa-map-marker"></i> Kaghan colony abbottabad</div>
                    </div>
                    <hr>
                    <div class="card-text text-sm-center">
                        <a href="#"><i class="fa fa-facebook pr-1"></i></a>
                        <a href="#"><i class="fa fa-twitter pr-1"></i></a>
                        <a href="#"><i class="fa fa-linkedin pr-1"></i></a>
                        <a href="#"><i class="fa fa-pinterest pr-1"></i></a>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>